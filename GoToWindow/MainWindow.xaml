<Window x:Class="GoToWindow.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:gotowindow="clr-namespace:GoToWindow"
        Title="Go To Window" Height="350" Width="525" WindowStartupLocation="CenterScreen" SourceInitialized="Window_SourceInitialized">
    <Grid>
        <ListView Name="windowsListView" Margin="10" ScrollViewer.HorizontalScrollBarVisibility="Disabled" MouseDoubleClick="windowsListView_MouseDoubleClick">
            <ListView.ItemTemplate>
                <DataTemplate>
                    <gotowindow:WindowListEntry />
                </DataTemplate>
            </ListView.ItemTemplate>
        </ListView>
    </Grid>
</Window>
